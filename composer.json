{
    "name": "radoslaw-malysa/slim4_starter",
    "description": "Starter Slim 4 project",
    "keywords": [
        "slim", "slim framwork", "psr7"
    ],
    "homepage": "https://github.com/radoslaw-malysa/slim4_starter",
    "type": "project",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require": {
        "php-di/php-di": "^6.0",
        "slim/php-view": "^3.0",
        "slim/psr7": "^0.6.0",
        "slim/slim": "^4.2"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
